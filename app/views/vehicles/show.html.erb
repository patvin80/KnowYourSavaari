    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['IsCertified', 'Count'],
          ['Yes',     <%= @yesCount %>],
          ['No',      <%= @noCount %>]
        ]);

        var options = {
          title: 'Inspection Results for <%= @vehicle.state_code %> - <%= @vehicle.state_number %> - <%= @vehicle.plate_letter %> - <%= @vehicle.plate_number %> '
        };

        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
    </script>

    <div id="main">
      <div class="5grid-layout">
        <div class="row main-row">
          <div class="8u">            
            <section class="left-content">
    					<h2><%= @vehicle.state_code %> - <%= @vehicle.state_number %> - <%= @vehicle.plate_letter %> - <%= @vehicle.plate_number %> </h2>
    					<p>Here are the results for the vehicle based upon earlier submissions.</p>
    					<div id="chart_div" style="width: 550px; height: 400px;"></div>
              <p>Feel free to <%= link_to "Search", vehicles_path %> another or if you would like to correct or enhance our results <%= link_to "Submit", new_vehicle_path %> a Record</p>
  				  </section>
			    </div>
          <div class="4u">
            <section>
              <h2>What Next?</h2>
              <ul class="small-image-list">
                <li>
                  <%= link_to image_tag("search.png", :class => "left"), vehicles_path %>
                  <h4>Search</h4>
                  <p>Search another vehicle using their license</p>
                </li>
                <li>
                  <%= link_to image_tag("add.png", :class => "left"), new_vehicle_path %>
                  <h4>Submit</h4>
                  <p>Submit a Record to strengthen our database. </p>
                </li>
              </ul>
            </section>
          
            <section>
              <h2>Enforcement Agencies?</h2>
              <div class="5grid">
                <div class="row">
                  <div class="6u">
                    <ul class="link-list">
                      <li><a href="http://peso.gov.in/contactus.aspx" target="_default">Chief Controller of Explosives</a></li>
                      <li><a href="http://mahapolice.gov.in/" target="_default">State Police</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </section>

          </div>
    		</div>
	    </div>
    </div>



